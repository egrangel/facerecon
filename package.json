{
  "name": "facial-recognition-backend",
  "version": "1.0.0",
  "description": "Backend application for facial recognition system with client management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:native": "node-gyp rebuild",
    "install:deps": "node scripts/install-deps.js",
    "install:native": "node scripts/install-native.js",
    "setup:native": "npm run install:deps",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "migrate": "typeorm migration:run -d src/config/database.ts",
    "migrate:revert": "typeorm migration:revert -d src/config/database.ts",
    "migration:generate": "typeorm migration:generate -d src/config/database.ts",
    "schema:sync": "typeorm schema:sync -d src/config/database.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t facial-recognition-api .",
    "docker:run": "docker run -p 3001:3001 facial-recognition-api",
    "deploy:build": "npm run build && npm run docker:build"
  },
  "keywords": [
    "facial-recognition",
    "backend",
    "api",
    "typescript",
    "express"
  ],
  "author": "Edimar Rangel",
  "license": "ISC",
  "dependencies": {
    "@tensorflow-models/face-detection": "^1.0.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.4.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.5",
    "@types/ws": "^8.5.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
